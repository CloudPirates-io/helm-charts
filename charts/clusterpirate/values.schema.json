{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "auth": {
            "type": "object",
            "properties": {
                "accessToken": {
                    "type": "string"
                },
                "existingSecret": {
                    "type": "string"
                },
                "existingSecretAccessTokenKey": {
                    "type": "string"
                }
            }
        },
        "cloudpiratesApi": {
            "type": "object",
            "properties": {
                "registerEndpoint": {
                    "type": "string"
                }
            }
        },
        "clusterPirate": {
            "type": "object",
            "properties": {
                "healthPort": {
                    "type": "integer"
                },
                "logLevel": {
                    "type": "string"
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "cache": {
                            "type": "object",
                            "properties": {
                                "host": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                },
                                "ttl": {
                                    "type": "integer"
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "updateIntervalSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "monitoring": {
                    "type": "object",
                    "properties": {
                        "resourceEventsEnabled": {
                            "type": "boolean"
                        },
                        "systemEventsEnabled": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "commonAnnotations": {
            "type": "object"
        },
        "commonLabels": {
            "type": "object"
        },
        "deployment": {
            "type": "object",
            "properties": {
                "extraEnvVars": {
                    "type": "array"
                },
                "probes": {
                    "type": "object",
                    "properties": {
                        "livenessProbe": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "failureThreshold": {
                                    "type": "integer"
                                },
                                "initialDelaySeconds": {
                                    "type": "integer"
                                },
                                "periodSeconds": {
                                    "type": "integer"
                                },
                                "successThreshold": {
                                    "type": "integer"
                                },
                                "timeoutSeconds": {
                                    "type": "integer"
                                }
                            }
                        },
                        "readinessProbe": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "failureThreshold": {
                                    "type": "integer"
                                },
                                "initialDelaySeconds": {
                                    "type": "integer"
                                },
                                "periodSeconds": {
                                    "type": "integer"
                                },
                                "successThreshold": {
                                    "type": "integer"
                                },
                                "timeoutSeconds": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "resources": {
                    "type": "object",
                    "properties": {
                        "limits": {
                            "type": "object",
                            "properties": {
                                "memory": {
                                    "type": "string"
                                }
                            }
                        },
                        "requests": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "imagePullSecrets": {
                    "type": "array"
                },
                "imageRegistry": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "imagePullPolicy": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "rbac": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "valkey": {
            "type": "object",
            "properties": {
                "auth": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "existingSecret": {
                            "type": "string"
                        },
                        "existingSecretPasswordKey": {
                            "type": "string"
                        },
                        "password": {
                            "type": "string"
                        }
                    }
                },
                "enabled": {
                    "type": "boolean"
                },
                "persistence": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                }
            }
        }
    }
}
