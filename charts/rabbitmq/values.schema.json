                    "type": "array"
                },
                "users": {
                    "type": "array"
                },
                "vhosts": {
                    "type": "array"
                }
            }
        },
        "extraEnv": {
            "type": "array"
        },
        "extraObjects": {
            "type": "array"
        },
        "extraPorts": {
            "type": "array"
        },
        "extraVolumeMounts": {
            "type": "array"
        },
        "extraVolumes": {
            "type": "array"
        },
