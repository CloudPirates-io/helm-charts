                "enabled": {
                    "type": "boolean"
                },
                "erlangCookie": {
                    "type": "string"
                },
                "existingErlangCookieKey": {
                    "type": "string"
                },
                "existingPasswordKey": {
                    "type": "string"
                },
                "existingSecret": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "clusterDomain": {
            "type": "string"
        },
        "commonAnnotations": {
            "type": "object"
        },
        "commonLabels": {
            "type": "object"
        },
        "config": {
            "type": "object",
            "properties": {
                "advancedConfiguration": {
                    "type": "string"
                },
                "extraConfiguration": {
                    "type": "string"
                },
                "logging": {
                    "type": "object",
                    "properties": {
                        "console": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "file": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "level": {
                            "type": "string"
                        }
                    }
                },
                "memoryHighWatermark": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "type": {
                            "type": "string"
                        },
                        "value": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "containerSecurityContext": {
            "type": "object",
            "properties": {
                "allowPrivilegeEscalation": {
                    "type": "boolean"
                },
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "drop": {
                            "type": "array",
