        "persistentVolumeClaimRetentionPolicy": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "whenDeleted": {
                    "type": "string"
                },
                "whenScaled": {
                    "type": "string"
                }
            }
