        "resources": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "amqpPort": {
                    "type": "integer"
                },
                "annotations": {
                    "type": "object"
                },
                "annotationsHeadless": {
                    "type": "object"
                },
                "distPort": {
                    "type": "integer"
                },
                "epmdPort": {
                    "type": "integer"
                },
                "managementPort": {
                    "type": "integer"
                },
                "trafficDistribution": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "statefulsetAnnotations": {
            "type": "object"
        },
        "statefulsetLabels": {
            "type": "object"
        },
        "tolerations": {
            "type": "array"
        },
        "topologySpreadConstraints": {
            "type": "array"
        }
    }
}
